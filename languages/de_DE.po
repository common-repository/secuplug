#: src/Lib/Admin.php:445
msgid "You can prevent to xmlrpc attacks."
msgstr "Sie können Angriffe auf xmlrpc verhindern."

#: src/Lib/Admin.php:451
msgid "Disable All XML-RPC Services"
msgstr "Alle XML-RPC-Dienste deaktivieren"

#: src/Lib/Admin.php:455 src/Lib/Admin.php:471 src/Lib/Admin.php:487
#: src/Lib/Admin.php:503 src/Lib/Admin.php:528 src/Lib/Admin.php:543
#: src/Lib/Admin.php:558 src/Lib/Admin.php:573 src/Lib/Admin.php:588
msgid "No"
